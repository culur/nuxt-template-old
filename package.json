{
  "name": "nuxt-template",
  "version": "0.1.0",
  "private": true,
  "description": "Nuxt Template - Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culur/nuxt-template.git"
  },
  "bugs": {
    "url": "https://github.com/culur/nuxt-template/issues"
  },
  "homepage": "https://github.com/culur/nuxt-template#readme",
  "author": "phamhongphuc <phamhongphuc.info@gmail.com>",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier . --write",
    "fix:stylelint": "stylelint **/*.{vue,tsx,pcss,css,scss} --fix",
    "fix:md": "markdownlint **/*.md -f -p .markdownlintignore",
    "test": "run-s test:*",
    "test:prettier": "prettier . --list-different",
    "test:stylelint": "stylelint **/*.{vue,tsx,pcss,css,scss}",
    "test:md": "markdownlint **/*.md -p .markdownlintignore"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.24.6",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@culur/prettier-config": "^0.2.0",
    "@culur/stylelint-config": "^0.2.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/tailwindcss": "4.2.0",
    "@types/node": "^16.0.0",
    "@types/tailwindcss": "^2.2.1",
    "core-js": "^3.15.2",
    "cspell": "^5.6.6",
    "markdownlint-cli": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "nuxt-vite": "0.1.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
