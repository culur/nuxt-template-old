    "test:prettier": "prettier . --list-different",
    "test:stylelint": "stylelint **/*.{vue,tsx,pcss,css,scss}",
    "test:md": "markdownlint **/*.md -p .markdownlintignore"
  },
  "engines": {
    "node": ">=14"
  },
